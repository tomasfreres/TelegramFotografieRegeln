---
import instagram from "../Content/instagram.json" assert { type: "json" };
---

<ul>
    {
        instagram.map((el) => {
            return (
                <li>
                    <a
                        href={`https://instagram.com/${el.instagram}`}
                        target="_blank"
                    >
                        {el.tgUsername != "" ? "@" + el.tgUsername : ""}
                        {el.name != ""
                            ? `${el.tgUsername == "" ? "" : "("}` +
                              el.name +
                              `${el.tgUsername == "" ? "" : ")"}`
                            : ""}
                    </a>
                </li>
            );
        })
    }
</ul>
